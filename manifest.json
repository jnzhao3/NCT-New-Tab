{
    "name": "NCT New Tab",
    "version": "1.1",
    "manifest_version": 2,
    "description": "Replaces each new tab with an NCT picture and quote.",
    "chrome_url_overrides": {
        "newtab": "page.html"
    },
    "icons": {
        "16": "smalljaemin.png",
        "32": "smalljaemin.png",
        "48": "smalljaemin.png",
        "128": "smalljaemin.png"
    },

    "browser_action": {
        "default_title": "NCT New Tab",
        "default_icon": "smalljaemin.png"
    }
}